module body Test is
    union Foo: Linear is
        case Value is
            value: Int32;
    end;

    function consume(foo: Foo): Unit is
        case foo of
            when Value(value: Int32) do
                skip;
        end case;
        return nil;
    end;

    function main(): ExitCode is
        let u: Foo := Value(value => 10);
        borrow u as uref in R do
            case uref of
                when Value(value: &[Int32, R]) do
                    skip;
            end case;
        end;
        consume(u);
        return ExitSuccess();
    end;
end module body.